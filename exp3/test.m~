ns = [1,2,10:10:100];

[X,Y] = meshgrid(ns, 0:0.1:100);

[M,N] = size(X);
Z = zeros(M,N);

% txtLegend = ;
for i = 1:M
    for j = 1:N
        Z(i,j) = erlangB(X(i,j),Y(i,j));
    end
end
legend();

% Plot a 2D graph with respect to A and PB
figure,
for i = 1:N
    plot(Y(:,i), Z(:,i));
            txtLegend = strcat('n=',num2str(i));
    hold on;
end
xlabel('x: A'), ylabel('y: P_B'),

% Plot a 3D-mesh.
figure, mesh(X,Y,Z);
xlabel('x: n'), ylabel('y: A'), zlabel('z: P_B');